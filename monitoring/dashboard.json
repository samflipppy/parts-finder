{
  "displayName": "PartsFinder Agent Health",
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "yPos": 0,
        "width": 12,
        "height": 2,
        "widget": {
          "title": "",
          "text": {
            "content": "## Key Performance Indicators",
            "format": "MARKDOWN"
          }
        }
      },
      {
        "yPos": 2,
        "width": 3,
        "height": 4,
        "widget": {
          "title": "Total Requests (24h)",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/agent_request_count\"",
                "aggregation": {
                  "alignmentPeriod": "86400s",
                  "perSeriesAligner": "ALIGN_SUM",
                  "crossSeriesReducer": "REDUCE_SUM"
                }
              }
            }
          }
        }
      },
      {
        "xPos": 3,
        "yPos": 2,
        "width": 3,
        "height": 4,
        "widget": {
          "title": "Part Found Rate (24h)",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilterRatio": {
                "numerator": {
                  "filter": "metric.type=\"logging.googleapis.com/user/agent_part_found\" metric.label.part_found=\"true\"",
                  "aggregation": {
                    "alignmentPeriod": "86400s",
                    "perSeriesAligner": "ALIGN_SUM",
                    "crossSeriesReducer": "REDUCE_SUM"
                  }
                },
                "denominator": {
                  "filter": "metric.type=\"logging.googleapis.com/user/agent_part_found\"",
                  "aggregation": {
                    "alignmentPeriod": "86400s",
                    "perSeriesAligner": "ALIGN_SUM",
                    "crossSeriesReducer": "REDUCE_SUM"
                  }
                }
              }
            },
            "thresholds": [
              { "value": 0.7, "color": "YELLOW", "direction": "BELOW" },
              { "value": 0.5, "color": "RED", "direction": "BELOW" }
            ]
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 2,
        "width": 3,
        "height": 4,
        "widget": {
          "title": "Avg Latency (24h)",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/agent_total_latency\"",
                "aggregation": {
                  "alignmentPeriod": "86400s",
                  "perSeriesAligner": "ALIGN_PERCENTILE_50",
                  "crossSeriesReducer": "REDUCE_MEAN"
                }
              }
            },
            "thresholds": [
              { "value": 5000, "color": "YELLOW", "direction": "ABOVE" },
              { "value": 10000, "color": "RED", "direction": "ABOVE" }
            ]
          }
        }
      },
      {
        "xPos": 9,
        "yPos": 2,
        "width": 3,
        "height": 4,
        "widget": {
          "title": "Avg Star Rating (24h)",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/user_feedback_rating\"",
                "aggregation": {
                  "alignmentPeriod": "86400s",
                  "perSeriesAligner": "ALIGN_PERCENTILE_50",
                  "crossSeriesReducer": "REDUCE_MEAN"
                }
              }
            },
            "thresholds": [
              { "value": 3.0, "color": "YELLOW", "direction": "BELOW" },
              { "value": 2.0, "color": "RED", "direction": "BELOW" }
            ]
          }
        }
      },
      {
        "yPos": 6,
        "width": 12,
        "height": 2,
        "widget": {
          "title": "",
          "text": {
            "content": "## Agent Quality & Accuracy",
            "format": "MARKDOWN"
          }
        }
      },
      {
        "yPos": 8,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "Confidence Distribution (high / medium / low)",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_request_count\"",
                    "aggregation": {
                      "alignmentPeriod": "3600s",
                      "perSeriesAligner": "ALIGN_SUM",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["metric.label.confidence"]
                    }
                  }
                },
                "plotType": "STACKED_BAR"
              }
            ],
            "yAxis": { "label": "Requests / hour" }
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 8,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "Part Found Rate Over Time",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilterRatio": {
                    "numerator": {
                      "filter": "metric.type=\"logging.googleapis.com/user/agent_part_found\" metric.label.part_found=\"true\"",
                      "aggregation": {
                        "alignmentPeriod": "3600s",
                        "perSeriesAligner": "ALIGN_SUM",
                        "crossSeriesReducer": "REDUCE_SUM"
                      }
                    },
                    "denominator": {
                      "filter": "metric.type=\"logging.googleapis.com/user/agent_part_found\"",
                      "aggregation": {
                        "alignmentPeriod": "3600s",
                        "perSeriesAligner": "ALIGN_SUM",
                        "crossSeriesReducer": "REDUCE_SUM"
                      }
                    }
                  }
                },
                "plotType": "LINE",
                "legendTemplate": "Part Found Rate"
              }
            ],
            "yAxis": { "label": "Rate (0-1)" }
          }
        }
      },
      {
        "yPos": 16,
        "width": 12,
        "height": 2,
        "widget": {
          "title": "",
          "text": {
            "content": "## Performance & Reliability",
            "format": "MARKDOWN"
          }
        }
      },
      {
        "yPos": 18,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "End-to-End Latency (P50 / P95 / P99)",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_total_latency\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_50"
                    }
                  }
                },
                "plotType": "LINE",
                "legendTemplate": "P50"
              },
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_total_latency\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_95"
                    }
                  }
                },
                "plotType": "LINE",
                "legendTemplate": "P95"
              },
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_total_latency\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_99"
                    }
                  }
                },
                "plotType": "LINE",
                "legendTemplate": "P99"
              }
            ],
            "yAxis": { "label": "Latency (ms)" }
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 18,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "Error Rate",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_error_count\"",
                    "aggregation": {
                      "alignmentPeriod": "300s",
                      "perSeriesAligner": "ALIGN_SUM"
                    }
                  }
                },
                "plotType": "LINE"
              }
            ],
            "yAxis": { "label": "Errors / 5 min" }
          }
        }
      },
      {
        "yPos": 26,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "Tool Latency by Tool Name (P50)",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_tool_latency\"",
                    "aggregation": {
                      "alignmentPeriod": "3600s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_50",
                      "crossSeriesReducer": "REDUCE_MEAN",
                      "groupByFields": ["metric.label.tool_name"]
                    }
                  }
                },
                "plotType": "STACKED_BAR"
              }
            ],
            "yAxis": { "label": "Latency (ms)" }
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 26,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "Tool Usage Count by Tool Name",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_tool_usage\"",
                    "aggregation": {
                      "alignmentPeriod": "3600s",
                      "perSeriesAligner": "ALIGN_SUM",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["metric.label.tool_name"]
                    }
                  }
                },
                "plotType": "STACKED_BAR"
              }
            ],
            "yAxis": { "label": "Calls / hour" }
          }
        }
      },
      {
        "yPos": 34,
        "width": 12,
        "height": 2,
        "widget": {
          "title": "",
          "text": {
            "content": "## LLM Usage & Cost",
            "format": "MARKDOWN"
          }
        }
      },
      {
        "yPos": 36,
        "width": 4,
        "height": 4,
        "widget": {
          "title": "LLM Requests (24h)",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/agent_request_by_model\"",
                "aggregation": {
                  "alignmentPeriod": "86400s",
                  "perSeriesAligner": "ALIGN_SUM",
                  "crossSeriesReducer": "REDUCE_SUM"
                }
              }
            }
          }
        }
      },
      {
        "xPos": 4,
        "yPos": 36,
        "width": 4,
        "height": 4,
        "widget": {
          "title": "Avg Tokens / Request",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/agent_total_tokens\"",
                "aggregation": {
                  "alignmentPeriod": "86400s",
                  "perSeriesAligner": "ALIGN_PERCENTILE_50",
                  "crossSeriesReducer": "REDUCE_MEAN"
                }
              }
            }
          }
        }
      },
      {
        "xPos": 8,
        "yPos": 36,
        "width": 4,
        "height": 4,
        "widget": {
          "title": "Avg Cost / Request",
          "scorecard": {
            "timeSeriesQuery": {
              "timeSeriesFilter": {
                "filter": "metric.type=\"logging.googleapis.com/user/agent_estimated_cost\"",
                "aggregation": {
                  "alignmentPeriod": "86400s",
                  "perSeriesAligner": "ALIGN_PERCENTILE_50",
                  "crossSeriesReducer": "REDUCE_MEAN"
                }
              }
            }
          }
        }
      },
      {
        "yPos": 40,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "Tokens Per Request (Input vs Output)",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_input_tokens\"",
                    "aggregation": {
                      "alignmentPeriod": "3600s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_50",
                      "crossSeriesReducer": "REDUCE_MEAN"
                    }
                  }
                },
                "plotType": "STACKED_BAR",
                "legendTemplate": "Input Tokens (median)"
              },
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_output_tokens\"",
                    "aggregation": {
                      "alignmentPeriod": "3600s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_50",
                      "crossSeriesReducer": "REDUCE_MEAN"
                    }
                  }
                },
                "plotType": "STACKED_BAR",
                "legendTemplate": "Output Tokens (median)"
              }
            ],
            "yAxis": { "label": "Tokens / request" }
          }
        }
      },
      {
        "xPos": 6,
        "yPos": 40,
        "width": 6,
        "height": 8,
        "widget": {
          "title": "Cost Per Request Over Time",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/agent_estimated_cost\"",
                    "aggregation": {
                      "alignmentPeriod": "3600s",
                      "perSeriesAligner": "ALIGN_PERCENTILE_50",
                      "crossSeriesReducer": "REDUCE_MEAN"
                    }
                  }
                },
                "plotType": "LINE",
                "legendTemplate": "Cost (USD)"
              }
            ],
            "yAxis": { "label": "Cost (USD) / request" }
          }
        }
      },
      {
        "yPos": 48,
        "width": 12,
        "height": 2,
        "widget": {
          "title": "",
          "text": {
            "content": "## User Feedback",
            "format": "MARKDOWN"
          }
        }
      },
      {
        "yPos": 50,
        "width": 12,
        "height": 8,
        "widget": {
          "title": "Feedback Ratings Over Time",
          "xyChart": {
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"logging.googleapis.com/user/user_feedback_count\"",
                    "aggregation": {
                      "alignmentPeriod": "3600s",
                      "perSeriesAligner": "ALIGN_SUM",
                      "crossSeriesReducer": "REDUCE_SUM",
                      "groupByFields": ["metric.label.rating"]
                    }
                  }
                },
                "plotType": "STACKED_BAR"
              }
            ],
            "yAxis": { "label": "Ratings / hour" }
          }
        }
      },
      {
        "yPos": 58,
        "width": 12,
        "height": 2,
        "widget": {
          "title": "",
          "text": {
            "content": "## Recent Logs",
            "format": "MARKDOWN"
          }
        }
      },
      {
        "yPos": 60,
        "width": 12,
        "height": 6,
        "widget": {
          "title": "Recent Agent Logs",
          "logsPanel": {
            "filter": "jsonPayload.message=\"agent_request_complete\" OR jsonPayload.message=\"user_feedback\" OR jsonPayload.message=\"agent_tool_call\"",
            "resourceNames": ["projects/${PROJECT_ID}"]
          }
        }
      }
    ]
  }
}
