{
  "name": "partsfinder-functions",
  "version": "1.0.0",
  "description": "PartsFinder Agent - Cloud Functions",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "seed": "tsc && node lib/seed.js",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "eval": "tsc && node lib/eval.js",
    "lint": "eslint src/"
  },
  "engines": {
    "node": "24"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.0.0",
    "firebase-admin": "^13.0.0",
    "firebase-functions": "^6.3.0",
    "genkit": "^1.0.0",
    "@genkit-ai/googleai": "^1.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "tsx": "^4.7.0",
    "dotenv": "^16.4.0"
  }
}
